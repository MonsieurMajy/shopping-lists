<% layout("./layouts/layout.eta") %>

<h1>Shopping Lists Manager 2023</h1>

<h2>Add a list</h2>

<form method="POST" action="/lists">
  Name: <input type="text" name="name" />
  <input type="submit" value="Create list!" />
</form>

<h2>Active lists</h2>

<ul>
<% it.lists.forEach((list) => { %>
  <li><%= list.name %></li>
<% }); %>
</ul>